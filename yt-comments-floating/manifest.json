{
  "name": "Youtube Popup Commenter",
  "description": "Pops up comments according to payback",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "16": "./icons/j-16.png",
    "32": "./icons/j-32.png",
    "48": "./icons/j-48.png",
    "128": "./icons/j-128.png"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "https://www.google.com/*", "storage"]
}
